pipeline{
    agent any

    stages{
        stage("build image"){
            steps {
                sh "docker build -t static_app/app:latest ."
                sh "docker tag static_app/app:latest adelinau/static_app:latest"
                sh "docker push adelinau/static_app:latest"
                sh "docker rmi static_app"
            }
        }
    }
}
    